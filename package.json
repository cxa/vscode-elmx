{
    "name": "elmx",
    "displayName": "elmx",
    "description": "Syntax highlighting for the Elm language extended with embedded HTML (elmx)",
    "version": "1.0.2",
    "publisher": "realazy",
    "repository": "https://github.com/cxa/vscode-elmx.git",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "elmx",
            "aliases": ["ElmX", "elmx"],
            "extensions": [".elmx"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "elmx",
            "scopeName": "source.elm.elmx",
            "path": "./syntaxes/elm.tmLanguage.json"
        }]
    }
}